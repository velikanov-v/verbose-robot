
@{
    ViewData["Title"] = "Tempor";
}

@model IEnumerable<TestProject.Models.Team>
@{
    ViewBag.Title = "Все группы";
}
<a asp-action="GroupRed">Добавить группу</a>
<table class="table">
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.GroupName</td>
            <td>
                <a asp-action="Details" asp-controller="Home" asp-route-id="@item.Id">Подробнее</a> |
                <a asp-action="Edit" asp-controller="Home" asp-route-id="@item.Id">Изменить</a> |
                <a asp-action="Delete" asp-controller="Home" asp-route-id="@item.Id">Удалить</a>
            </td>
        </tr>
    }
</table>
<p class="nav-item">
<p style="text-align: center">
    <button align="center"><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="TestView">TEst</a></button>
</p>
    </p>