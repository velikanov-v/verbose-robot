

@{
    ViewData["Title"] = "Delete";
}

@model SecondProject.Models.Group
@{
    ViewBag.Title = "Удаление группы";
}

<div>
    <dl class="dl-horizontal">
        <dt>Группа</dt>
        <dd>
            Группа - @Html.DisplayFor(model => model.GroupNumber)
        </dd>

        <dt>Учитель</dt>
        <dd>
            @Html.DisplayFor(model => model.Teacher)
        </dd>
                
    </dl>

    <div>
        <form asp-controller="Home" asp-action="Delete" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Удалить" />
            </div>
        </form>
    </div>
</div>
